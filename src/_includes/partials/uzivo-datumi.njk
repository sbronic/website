<!-- sidebar -->
<div class="ttm-row sidebar ttm-sidebar-right ttm-bgcolor-white clearfix">
	<div class="container">
		<!-- row -->
		<div class="row">
			<div class="col-xl-9 col-lg-8 content-area">

				{% if seminari != 'prazno' %}

				{% for item in pagination.items %}

				{% if item.popunjen %}

				<div class="featured-imagebox featured-imagebox-post style1">
					<div class="featured-content featured-content-post" style="padding: 20px;!important">
						<div class="row">
							<div class="col-sm-2" style="max-width: 10.666667%;!important">
								<div class="icon-box">
									<div class="ttm-icon ttm-icon_element-fill ttm-icon_element-color-darkgrey ttm-icon_element-size-sm ttm-icon_element-style-rounded" style="background-color:lightgrey;margin-bottom: 0px;!important">
										<div class="ttm-box-event-date">
											<span class="ttm-entry-date">
												<time class="entry-date" datetime="{{ item.datumseminara }}" style="margin-top:10px;">{{ item.datumseminara | datumDan }}<span class="entry-month">{{ item.datumseminara | datumMjesec }}</span></time>
											</span>
										</div>
									</div>
								</div>
							</div>
							<div class="col-sm-7">
								<div class="post-meta" style="border-bottom: solid 1px;">
									<span class="ttm-meta-line"><i class="fa fa-clock-o"></i>{{ item.vrijeme }}</span>
									{% if item.oblik === "Seminar" %}
									<span class="ttm-meta-line"><i class="fa fa-map-marker"></i>{{ item.lokacijanaziv }}, {% if item.dvorana %}Dvorana {{ item.dvorana }},{% endif %} {{ item.lokacijaadresa }}, {{ item.lokacijagrad }}</span>
									{% else %}
									<span class="ttm-meta-line"><i class="fa fa-map-desktop"></i>Webinar</span>
									{% endif %}
								</div>
								<div class="post-title featured-title" style="margin-top: 10px;">
									<h6><a href="/edukacije/seminari-uzivo/{{ item.kategorijaslug }}/{{ item.seminarnazivslug }}" style="color:lightgrey!important"><del>{{ item.seminarnaziv }}</del></a></h6>
								</div>
							</div>
							<div class="col-sm-3">
								<div class="ttm-btn-box" style="right:0px;width:100%;margin-top:20px;">
									<h5 style="color: lightgrey!important;">POPUNJEN</h5>
								</div>
							</div>
						</div>
					</div>
				</div><!-- featured-imagebox-post end-->

				{% else %}

				<div class="featured-imagebox featured-imagebox-post style1">
					<div class="featured-content featured-content-post" style="padding: 20px;!important">
						<div class="row">
							<div class="col-sm-2" style="max-width: 10.666667%;!important">
								<div class="icon-box">
									<div class="ttm-icon ttm-icon_element-fill ttm-icon_element-color-darkgrey ttm-icon_element-size-sm ttm-icon_element-style-rounded" style="background-color:#8FBC94;margin-bottom: 0px;!important">
										<div class="ttm-box-event-date">
											<span class="ttm-entry-date">
												<time class="entry-date" datetime="{{ item.datumseminara }}" style="margin-top:10px;">{{ item.datumseminara | datumDan }}<span class="entry-month">{{ item.datumseminara | datumMjesec }}</span></time>
											</span>
										</div>
									</div>
								</div>
							</div>
							<div class="col-sm-7">
								<div class="post-meta" style="border-bottom: solid 1px;">
									<span class="ttm-meta-line"><i class="fa fa-clock-o"></i>{{ item.vrijeme }}</span>
									{% if item.oblik === "Seminar" %}
									<span class="ttm-meta-line"><i class="fa fa-map-marker"></i>{{ item.lokacijanaziv }}, Dvorana {{ item.dvorana }}, {{ item.lokacijaadresa }}, {{ item.lokacijagrad }}<i data-toggle="tooltip" data-placement="top" title="Seminar vodi predavač pred polaznicima u dvorani" class="fa fa-info-circle" style="margin-left:5px;color:lightslategrey" aria-hidden="true"></i></span>
									{% else %}
									<span class="ttm-meta-line"><i class="fa fa-laptop"></i>Webinar<i data-toggle="tooltip" data-placement="top" title="Seminar vodi predavač on-line putem Zooma" class="fa fa-info-circle" style="margin-left:5px;color:lightslategrey" aria-hidden="true"></i></span>
									{% endif %}
								</div>
								<div class="post-title featured-title" style="margin-top: 10px;">
									<h6><a href="/edukacije/seminari-uzivo/{{ item.kategorijaslug }}/{{ item.seminarnazivslug }}">{{ item.seminarnaziv }}</a></h6>
								</div>
							</div>
							<div class="col-sm-3">
								<div class="ttm-btn-box" style="right:0px;width:100%;margin-top:20px;">
									<h5 style="color:#88402C;margin-bottom:0px">{{ item.seminarcijenaEUR }}</h5>
									<p>{{ item.seminarcijena }}</p>
								</div>
							</div>
						</div>
					</div>
				</div><!-- featured-imagebox-post end-->

				{% endif %}

				{% endfor %}

				{% include "partials/pagination-block.njk" %}

			</div>

			{% else %}

			<!-- post -->
			<article class="post ttm-blog-classic clearfix">
				<!-- ttm-blog-classic-content -->
				<div class="ttm-blog-classic-content">
					<div class="ttm-post-entry-header">
						<header class="entry-header">
							<h2 class="entry-title">Trenutno nemamo dostupnih datuma seminara u ovoj kategoriji.</h2>
						</header>
					</div>
				</div><!-- ttm-blog-classic-content end -->
			</article><!-- post end -->
		</div>

		{% endif %}

		{% include "partials/uzivo-stupac.njk" %}

	</div><!-- row end -->
</div>
</div>
<!-- sidebar end -->