<!--course-section-->
<section class="ttm-row course-section ttm-bgcolor-white clearfix">
	<div class="container">
		<!-- row -->
		<div class="row">
			<div class="col-lg-12">
				<!-- section title -->
				<div class="section-title title-style-center_text">
					<div class="title-header">
						<h5>LAKO DO NOVIH ZNANJA</h5>
						<h1 class="title">Najtraženije edukacije</h1>
					</div>
					<div class="heading-seperator"><span></span></div>
				</div><!-- section title end -->
			</div>
		</div><!-- row end -->
		<!-- slick_slider -->
		<div class="row slick_slider" data-slick='{"slidesToShow": 3, "slidesToScroll": 2, "arrows":false, "autoplay":true, "centerMode":true, "centerPadding":0, "infinite":true, "responsive": [{"breakpoint":1100,"settings":{"slidesToShow": 3}} , {"breakpoint":777,"settings":{"slidesToShow": 2}},{"breakpoint":590,"settings":{"slidesToShow": 1}}]}'>

			{% for item in seminari.svepopis %}
			{% if item.popularan %}
			
			<div class="ttm-box-col-wrapper col-lg-4">
				<!-- featured-imagebox-course -->
				<div class="featured-imagebox featured-imagebox-course">
					<div class="ttm-post-thumbnail featured-thumbnail">
						<img class="img-fluid lazyload" data-src="{{ site.cdn }}/resize=fit:clip,width:526,height:190/output=format:webp/{{ item.fotografija }}" alt="{{ item.title }}">
					</div>
					<div class="featured-content featured-content-post">
						<div class="featured-content-post-inner" style="padding: 23px 20px 0px 20px!important;">
							<div class="post-title featured-title" style="min-height:55px;">
								<h5><a href="/edukacije/seminari-i-radionice/{{ item.kategorijaslug }}/{{ item.titleslug }}/">{{ item.title }}</a></h5>
							</div>
							<div class="post-desc featured-desc">
								<p style="min-height:150px;">{{ item.excerpt | truncate(190) }}</p>
								<br />
								<span style="color:#88402C;font-size:14px;font-style:italic;">Seminar</span>
							</div>
						</div>
						<div class="ttm-course-box-meta">
							<span class="ttm-lp-price">
								<ins><strong>{{ item.cijena }}</strong> ({{item.cijenaEUR}})</ins>
							</span>
						</div>
					</div>
				</div><!-- featured-imagebox-course end-->
			</div>
			
			{% endif %}
			{% endfor %}

			{% for item in online.sve %}
			{% if item.popularan %}

			<div class="ttm-box-col-wrapper col-lg-4">
				<!-- featured-imagebox-course -->
				<div class="featured-imagebox featured-imagebox-course">
					<div class="ttm-post-thumbnail featured-thumbnail">
						<img class="img-fluid lazyload" data-src="{{ site.cdn }}/resize=fit:clip,width:526,height:190/output=format:webp/{{ item.photo }}" alt="{{ item.title }}">
					</div>
					<div class="featured-content featured-content-post">
						<div class="featured-content-post-inner" style="padding: 23px 20px 0px 20px!important;">
							<div class="post-title featured-title" style="min-height:55px;">
								<h5><a href="/edukacije/online-edukacije/{{ item.categoryslug }}/{{ item.titleslug }}/">{{ item.title }}</a></h5>
							</div>
							<div class="post-desc featured-desc">
								<p style="min-height:150px;">{{ item.excerpt | truncate(190) }}</p>
								<br />
								<span style="color:#88402C;font-size:14px;font-style:italic;">
									{% if item.vrsta == "e_Zbirke"%}e-Zbirka{% endif %}
									{% if item.vrsta == "Video_seminari"%}Video seminar{% endif %}
									{% if item.vrsta == "Audio_seminari"%}Audio seminar{% endif %}
									{% if item.vrsta == "Paketi"%}Paket{% endif %}
									{% if item.vrsta == "Webinari"%}Webinar{% endif %}
									{% if item.vrsta == "Zoom_meeting"%}Zoom meeting uživo{% endif %}
									{% if item.vrsta == "Zoom_snimka"%}Zoom meeting snimka{% endif %}
								</span>
							</div>
						</div>
						<div class="ttm-course-box-meta">
							<ins><strong>{{ item.cijena }}</strong> ({{item.cijenaEUR}})</ins>
						</div>
					</div>
				</div><!-- featured-imagebox-course end-->
			</div>

			{% endif %}
			{% endfor %}

			{% for item in online.zbirke %}
			{% if item.popularan %}
			
			<div class="ttm-box-col-wrapper col-lg-4">
				<!-- featured-imagebox-course -->
				<div class="featured-imagebox featured-imagebox-course">
					<div class="ttm-post-thumbnail featured-thumbnail">
						<img class="img-fluid lazyload" data-src="{{ site.cdn }}/resize=fit:clip,width:526,height:400/output=format:webp/{{ item.photo }}" alt="{{ item.title }}">
					</div>
					<div class="featured-content featured-content-post">
						<div class="featured-content-post-inner" style="padding: 23px 20px 0px 20px!important;">
							<div class="post-title featured-title" style="min-height:55px;">
								<h5><a href="/edukacije/online-edukacije/{{ item.categoryslug }}/{{ item.titleslug }}/">{{ item.title }}</a></h5>
							</div>
							<div class="post-desc featured-desc">
								<p style="min-height:150px;">{{ item.excerpt | truncate(190) }}</p>
								<br />
								<span style="color:#88402C;font-size:14px;font-style:italic;">e-Zbirka</span>
							</div>
						</div>
						<div class="ttm-course-box-meta">
							<div class="ttm-enrolled">
								<span class="ttm-count ttm-meta-line"><i class="fa fa-tag" aria-hidden="true"></i>{{ item.category }}</span>
							</div>
							<span class="ttm-lp-price"><ins>{{ item.cijena }}</ins></span>
						</div>
					</div>
				</div><!-- featured-imagebox-course end-->
			</div>
			
			{% endif %}
			{% endfor %}

		</div>
		<!-- row -->
		<div class="row">
			<div class="col-lg-12">
				<div class="mt-30 res-991-mt-15 mb_25 text-center">
					<p>Slobodno nas <u><a href="/kontakt"><strong>kontaktirajte</strong></a></u> za detalje ili <u><a href="/edukacije/online-edukacije/"><strong>pogledajte sve on-line edukacije.</strong></a></u></p>
				</div>
			</div>
		</div><!-- row end -->
	</div>
</section>
<!--course-section-->