---js
{
  layout: "layouts/page.njk",
  title: "Seminari i radionice",
  description: "Prodaja, marketing i odnosi s javnošću",
  banner: "seminari.jpg",
  navID: "sem-pmoj",
  nav2ID: "datumi",
  permalink: "edukacije/seminari-uzivo/prodaja-marketing-i-odnosi-s-javnoscu/datumi-odrzavanja{% if pagination.pageNumber > 0 %}/stranica/{{ pagination.pageNumber + 1}}{% endif %}/index.html",
  pagination: {
    data: "seminari-uzivo.datumi",
    size: 10,
	  alias: "seminari",
    before: function(data) {
      const sviobjekti = [];
	  
      for (const obj of data) {
        if ( obj.kategorijaslug === "prodaja-marketing-i-odnosi-s-javnoscu" ) {
            sviobjekti.push(obj);
        }
	    }
      if (!sviobjekti.length) {
        sviobjekti.push("prazno")
      }

      return sviobjekti;
    }
  },
  eleventyComputed: {
    page_title: "Datumi održavanja seminara - Prodaja, marketing i odnosi s javnošću {% if pagination.pages | length != 1 %}({{ pagination.pageNumber + 1 }}/{{ pagination.pages | length }}){% endif %}",
    page_description: "Datumi održavanja seminara - Prodaja, marketing i odnosi s javnošću {% if pagination.pages | length != 1 %}({{ pagination.pageNumber + 1 }}/{{ pagination.pages | length }}){% endif %}"
  }
}
