---js
{
  layout: "layouts/page.njk",
  title: "Seminari uživo",
  description: "Prodaja, marketing i odnosi s javnošću",
  banner: "seminari.jpg",
  navID: "sem-pmoj",
  nav2ID: "popis",
  permalink: "edukacije/seminari-uzivo/prodaja-marketing-i-odnosi-s-javnoscu/popis{% if pagination.pageNumber > 0 %}/stranica/{{ pagination.pageNumber + 1}}{% endif %}/index.html",
  pagination: {
    data: "seminari-uzivo.popis",
    size: 10,
	  alias: "seminari",
    before: function(data) {
      const sviobjekti = [];
	  
      for (const obj of data) {
        if ( obj.kategorijaslug === "prodaja-marketing-i-odnosi-s-javnoscu" ) {
            sviobjekti.push(obj);
        }
	    }
      if (!sviobjekti.length) {
        sviobjekti.push("prazno")
      }

      return sviobjekti;
    }
  },
  eleventyComputed: {
    page_title: "Seminari i radionice - Prodaja, marketing i odnosi s javnošću {% if pagination.pages | length != 1 %}({{ pagination.pageNumber + 1 }}/{{ pagination.pages | length }}){% endif %}",
    page_description: "Seminari i radionice - Prodaja, marketing i odnosi s javnošću {% if pagination.pages | length != 1 %}({{ pagination.pageNumber + 1 }}/{{ pagination.pages | length }}){% endif %}"
  }
}
